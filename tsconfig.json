{
	"compilerOptions": {
		"target": "es5",
		"module": "es6",
		"lib": ["ESNext", "dom"],
		"jsx": "react",
		"sourceMap": true,
		"strict": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"moduleResolution": "node",
		"forceConsistentCasingInFileNames": true,
		"esModuleInterop": true
	},
	"include": ["src"]
}
